<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ionicframework.appfk689212" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Keyboard">
        <param name="android-package" value="io.ionic.keyboard.IonicKeyboard" />
        <param name="onload" value="true" />
    </feature>
    <splash src="splash.png" />
    <icon src="icon.png" />
    <splash density="port-hdpi" platform="android" src="assets/splash/res-long-port-hdpi/default.png" />
    <splash density="port-ldpi" platform="android" src="assets/splash/res-long-port-ldpi/default.png" />
    <splash density="port-mdpi" platform="android" src="assets/splash/res-long-port-mdpi/default.png" />
    <splash density="port-xhdpi" platform="android" src="assets/splash/res-long-port-xhdpi/default.png" />
    <splash density="port-xxhdpi" platform="android" src="assets/splash/res-long-port-xxhdpi/default.png" />
    <splash density="port-xxxhdpi" platform="android" src="assets/splash/res-long-port-xxxhdpi/default.png" />
    <splash density="port-hdpi" platform="android" src="assets/splash/res-notlong-port-hdpi/default.png" />
    <splash density="port-ldpi" platform="android" src="assets/splash/res-notlong-port-ldpi/default.png" />
    <splash density="port-mdpi" platform="android" src="assets/splash/res-notlong-port-mdpi/default.png" />
    <splash density="port-xhdpi" platform="android" src="assets/splash/res-notlong-port-xhdpi/default.png" />
    <splash density="port-xxhdpi" platform="android" src="assets/splash/res-notlong-port-xxhdpi/default.png" />
    <splash density="port-xxxhdpi" platform="android" src="assets/splash/res-notlong-port-xxxhdpi/default.png" />
    <name>familienKarte</name>
    <description>
        An Ionic Framework and Cordova project.
    </description>
    <author email="sven.hoffmann@hcmedia.de" href="http://www.hcmedia.de/">
      Sven Hoffmann, hc media GmbH
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="webviewbounce" value="false" />
    <preference name="UIWebViewBounce" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="2000" />
    <preference name="FadeSplashScreenDuration" value="2000" />
    <preference name="android-minSdkVersion" value="16" />
    <preference name="BackupWebStorage" value="none" />
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
</widget>
