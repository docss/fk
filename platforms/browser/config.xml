<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ionicframework.appfk689212" version="1.0.0" versionCode="19" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <name>Familienkarte</name>
    <description>
        Familienkarte im Landkreis Goslar
    </description>
    <author email="sven.hoffmann@hcmedia.de" href="http://www.hcmedia.de/">
      Sven Hoffmann, hc media GmbH
    </author>
